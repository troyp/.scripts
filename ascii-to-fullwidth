#!/usr/bin/env python
# -*- coding: utf-8 -*-

import sys

for line in sys.stdin:
    for char in line:
        if char == " ":
            sys.stdout.write("ã€€")    # ideographic space
        else:
            code_pt = ord(char)
            sys.stdout.write(unichr(code_pt + 0xFEE0))
    sys.stdout.write("\n")


